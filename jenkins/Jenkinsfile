#!groovy
node { // <1>
    echo 'Hello from Jenkinsfile' // <2> 
    stage('Build') {
        sh 'echo \'hallooooo\''
        sh 'pwd'
        checkout scm
        sh './jenkins/test.sh'
    }
}
node { // <2>
    echo 'Hello from node 2' // <2> 
    stage('Build') {
        sh 'echo \'running exec_atest.sh\''
        sh './test/exec_atest.sh'
    }
}


